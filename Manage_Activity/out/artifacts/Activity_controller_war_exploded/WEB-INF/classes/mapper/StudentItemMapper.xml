<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mapper.StudentItemMapper" >
<select id="getPageBean" parameterType="pojo.PageBean" resultType="pojo.Student">
    SELECT
    snum,
    sname,
    sfaculty,
    smajor,
    sclass,
    stelphone,
    sstatus
    FROM
    student
    limit
    #{start},#{pageSize};
</select>
    <select id="getStudentTotalCount" resultType="java.lang.Integer">
       select count(*) from student ;
    </select>
</mapper>